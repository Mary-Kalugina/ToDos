        "jest-environment-jsdom": "^29.7.0",
        "mini-css-extract-plugin": "^2.6.1",
        "postcss": "^8.4.14",
        "postcss-loader": "^7.0.1",
        "postcss-preset-env": "^7.7.2",
        "sass": "^1.53.0",
        "sass-loader": "^13.0.2",
        "style-loader": "^3.3.1",
        "svg-inline-loader": "^0.8.2",
        "svg-url-loader": "^8.0.0",
        "terser-webpack-plugin": "^5.3.3",
